{
    "name": "controleonline/speed-up-essentials",
    "description": "HTML Validator. Beaultify your HTML on Dev and Compress on Production",
    "license": "BSD-3-Clause",
    "homepage": "http://controleonline.com/",
    "keywords": [
        "html",
        "compress",
        "minify"
    ],
    "autoload": {
        "psr-4": {
            "SpeedUpEssentials\\": "module/SpeedUpEssentials/src/SpeedUpEssentials"
        },
        "classmap": [
            "module/SpeedUpEssentials/Module.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "component-dir": "public/components"
    },
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "~2.0",
        "tubalmartin/cssmin": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:controleonline/speed-up-essentials.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/tubalmartin/YUI-CSS-compressor-PHP-port"
        }
    ]
}
